-- Sounds

local Bark = Instance.new("Sound", Head)

Bark.Name = "Bark"
Bark.SoundId = "http://www.roblox.com/asset/?id=144016898"
Bark.Volume = 1

local Wow = Instance.new("Sound", Head)

Wow.Name = "Wow"
Wow.SoundId = "http://www.roblox.com/asset/?id=257361046"
Wow.Volume = 1

-- Movement

local Walking = false
local Jumping = false
local Sin = 0

Plr.Character.Humanoid.FreeFalling:Connect(function(Active)

	if Active == true and not Jumping then
		
		Jumping = true
		
		if math.sin(Sin) > 0 then
			
			Sin = -1.5708
			
			LeftHip.DesiredAngle = 1
			RightHip.DesiredAngle = 1
			LeftShoulder.DesiredAngle = 1
			RightShoulder.DesiredAngle = 1
			
		else
			
			Sin = 1.5708
			
			LeftHip.DesiredAngle = -1
			RightHip.DesiredAngle = -1
			LeftShoulder.DesiredAngle = -1
			RightShoulder.DesiredAngle = -1
			
		end
		
	elseif Active == false and Jumping then
		
		Jumping = false
		
	end

end)

Plr.Character.Humanoid.Running:Connect(function(Speed)

	if Speed == 0 and Walking then

		Walking = false

	end

	if Speed ~= 0 and not Walking then

		Walking = true

		Sin = 0

		while Walking do

			wait()

			Sin += Plr.Character.Humanoid.WalkSpeed * 0.0125

			if not Jumping then

				LeftHip.DesiredAngle = math.sin(Sin)
				RightHip.DesiredAngle = math.sin(Sin)
				LeftShoulder.DesiredAngle = math.sin(Sin)
				RightShoulder.DesiredAngle = math.sin(Sin)

			end

		end

		if not Jumping then

			LeftHip.DesiredAngle = 0
			RightHip.DesiredAngle = 0
			LeftShoulder.DesiredAngle = 0
			RightShoulder.DesiredAngle = 0

		end

	end

end)

-- Death

Plr.Character.Humanoid.Died:Connect(function()
	
	for _, Part in Doge:GetChildren() do
		
		if Part:IsA("BasePart") then
			
			Part.CanCollide = true
			Part:BreakJoints()
			Part.RotVelocity = Vector3.new(math.random(-15,15),math.random(-15,15),math.random(-15,15))
			
		end
		
	end
	
end)

-- Tools

local Scare = Instance.new("Tool",Plr.Backpack)

Scare.Name = "Scare"
Scare.TextureId = "http://www.roblox.com/asset/?id=15332139712"
Scare.ToolTip = "Scare people and make them run away, Rotate your head to control them."
Scare.CanBeDropped = false
Scare.RequiresHandle = false

Scare.Activated:Connect(function()
	
	Scare.Enabled = false
	
	Bark:Play()
	
	local RayParams = RaycastParams.new()
	RayParams.FilterType = Enum.RaycastFilterType.Exclude
	RayParams.FilterDescendantsInstances = {Doge}

	local Result = game.Workspace:Raycast(Head.Position, Head.CFrame.LookVector * 50, RayParams)

	if Result and Result.Instance.Parent:FindFirstChildOfClass("Humanoid") or Result and Result.Instance.Parent.Parent:FindFirstChildOfClass("Humanoid") then
		
		local Victim = nil
		
		if Result.Instance.Parent:FindFirstChildOfClass("Humanoid") then
			
			Victim = Result.Instance.Parent
			
		else
			
			Victim = Result.Instance.Parent.Parent
			
		end
		
		local Sound = Instance.new("Sound",Victim.Head)
		
		Sound.SoundId = "rbxassetid://6108565657"
		Sound:Play()
		Sound.Ended:Connect(function() Sound:Destroy() end)
		
		local Try,Error = pcall(function()
			
			game.Chat:Chat(Victim,"",Enum.ChatColor.White)
			
		end)
		
		if not Try then
			
			game.Chat:Chat(Victim.Head,"",Enum.ChatColor.White)

		end
		
		Victim.Humanoid.WalkSpeed *= 1.5
		
		for i=1,100 do
			
			wait(0.01)
			
			Victim.Humanoid:Move(Head.CFrame.LookVector * 50000,false)
			
		end
		
		Victim.Humanoid.WalkSpeed /= 1.5
		
		wait(2)
		
		Scare.Enabled = true
		
	else
		
		wait(2)
		
		Scare.Enabled = true
		
	end
	
end)

local Wowsers = Instance.new("Tool",Plr.Backpack)

Wowsers.Name = "WOW!"
Wowsers.ToolTip = "Just lets you spam wow."
Wowsers.CanBeDropped = false
Wowsers.RequiresHandle = false

Wowsers.Activated:Connect(function()

	local Clone = Wow:Clone()
	
	Clone.Parent = Head
	
	Clone.PlaybackSpeed = math.random(90,110) / 100

	Clone:Play()
	
	Clone.Ended:Connect(function() Clone:Destroy() end)

end)
